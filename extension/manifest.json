{
  "manifest_version": 2,
  "name": "github-material-icon-theme",
  "version": "4.33.0",
  "description": "[description]",
  "icons": {
    "16": "./logo.png",
    "48": "./logo.png",
    "128": "./logo.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "index.mjs"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "./logo.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "github@materialicontheme.com",
      "strict_min_version": "48.0"
    }
  }
}
